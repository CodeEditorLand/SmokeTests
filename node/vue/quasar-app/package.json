{
	"browserslist": [
		"last 1 version, not dead, ie >= 11"
	],
	"dependencies": {
		"@quasar/extras": "1.16.11",
		"@tauri-apps/api": "1.5.3",
		"core-js": "3.36.1",
		"quasar": "2.15.2"
	},
	"description": "A Quasar Framework app",
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@quasar/app": "3.3.3",
		"execa": "8.0.1",
		"pkg": "5.8.1"
	},
	"name": "quasar-app",
	"productName": "Quasar App",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
